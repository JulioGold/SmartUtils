<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;net452;net46;net461</TargetFrameworks>
    <PackageOutputPath Condition="'$(Configuration)' == 'Release' ">$(SolutionDir)\NuGetPkg\</PackageOutputPath>
    <GeneratePackageOnBuild Condition="'$(Configuration)' == 'Release' ">true</GeneratePackageOnBuild>
    <title>SmartUtils</title>
    <PackageId>SmartUtils</PackageId>
    <Description>Utils things to use with .NET.</Description>
    <Authors>JulioGold</Authors>
    <Owners>JulioGold</Owners>
    <PackageLicenseUrl>https://raw.githubusercontent.com/JulioGold/SmartUtils/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/JulioGold/SmartUtils</PackageProjectUrl>
    <Copyright>Copyright 2018</Copyright>
    <PackageTags>Utils things to use with .NET.</PackageTags>
    <Version>0.0.13</Version>
    <RepositoryUrl>https://github.com/JulioGold/SmartUtils</RepositoryUrl>
  </PropertyGroup>

</Project>
